{
    "version": 2,
    "builds": [
      {
        "src": "api/spam.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/control.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/spam", "dest": "/api/spam.js" },
      { "src": "/api/control", "dest": "/api/control.js" },
      { "src": "/", "dest": "/public/index.html" }
    ]
  }
  